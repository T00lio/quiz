  "scripts": {
    "build": "npx tsc",
    "start": "npx tsc && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "initialize": "npm run setup-database && npm run migrate-database",
    "setup-database": "npm run create-database && npm run setup-user-permissions",
    "install-postgresql": "sudo apt-get install postgresql",
    "create-database": "sudo -u postgres createdb quizcompleted",
    "setup-user-permissions": "sudo -u postgres psql -c 'CREATE USER tulio WITH PASSWORD ''12345''; GRANT ALL PRIVILEGES ON DATABASE quizcompleted TO myuser;'",
    "migrate-database": "prisma migrate dev --preview-feature"
  },
